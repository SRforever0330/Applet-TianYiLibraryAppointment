page{
  background-color: #f4f4f4
}

.container{
  background-color: #fff;
  display: flex;
  margin:auto;
  margin-top: 40rpx;
  width: 90%;
  height: 90vh;
  box-shadow: 10rpx 10rpx #8888;
  align-items: center;
  flex-direction: column;
}
.image image{
  margin: auto;
  margin-top: 40rpx;
  width: 340rpx;
  height: 240rpx;
}
.info{
  width: 100%;
  margin-top: 30rpx;
}
.title{
  padding:16px 16px 8px;
  font-size:14px;font-size:14px;
  line-height:16px;line-height:16px;
  color:#969799;}
.item{
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #8888;
}
.content{
  padding:16px 16px 8px;
  font-size:14px;font-size:14px;
  line-height:16px;line-height:16px;
}
.sign{
  display: flex;
  justify-content: space-between;
}
.button{
  margin: 30rpx;
  width: 200rpx;
  height: 100rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.signIn{
  background-color: green;
  font-size: 40rpx;
  color:white;
}
.cancel{
  background-color: red;
  font-size: 40rpx;
  color:white;
}
.signOut{
  background-color: blue;
  font-size: 40rpx;
  color:white;
}
.one{
  /* 已预约 */
  color:green;
}
.two{
  /* 已签到 */
  color: blue;
}
.three{
  /* 已过期 */
  color:red;
}
.none{
  width: 100%;
  height: 100vh;
}