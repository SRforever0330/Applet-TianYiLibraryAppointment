page{
  background-color: #f4f4f4
}
.container{
  background-color: #fff;
  display: flex;
  margin:auto;
  margin-top: 40rpx;
  width: 90%;
  height: 90vh;
  box-shadow: 10rpx 10rpx #8888;
  align-items: left;
  flex-direction: column;
  position: relative;
}
.user_info_wrap{
  z-index: 2;
  width: 100%;
  overflow: hidden;
  position: relative;
}
.user_info_wrap .user_img_wrap{
  z-index: 2;
  position: relative;
  
}
.user_info_wrap .user_img_wrap .user_bg{
  position: relative;
  z-index: 2;
  width: 110%;
  justify-content: left;
  height: 40vh;
  filter: blur(10rpx);
}
.user_info_wrap .user_img_wrap .user_info{
  z-index: 2;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 30%;
  text-align: center;
}
.user_info_wrap .user_img_wrap .user_info .user_icon{
  position: relative;
  z-index: 2;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_info_wrap .user_img_wrap .user_info .user_name{
  position: relative;
  z-index: 2;
  color: #fff;
  margin-top: 40rpx;
  font-size: 30rpx;
}
