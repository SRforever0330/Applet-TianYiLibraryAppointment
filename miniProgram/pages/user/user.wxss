.content{
  margin-top: 30rpx;
  z-index: 1;
  
}
.appointment{
  z-index: 2;
  position: relative;
  margin-top:30rpx;
  display: flex;
  align-items: center;
  height:100rpx;
  font-size: 35rpx;
  padding:10rpx;
  padding-left: 30rpx;
  margin: 15rpx;
  color: #888;
  background-color: #f4f4f4;
  border:1rpx dotted #eb4450;
  border-radius: 10rpx;
}
.feedback{
  z-index: 1;
  position: relative;
  display: flex;
  align-items: center;
  height:100rpx;
  font-size: 35rpx;
  padding:10rpx;
  padding-left: 30rpx;
  margin: 15rpx;
  color: #888;
  background-color: #f4f4f4;
  border:1rpx dotted #eb4450;
  border-radius: 10rpx;
}
.recommend{
  z-index: 2;
  position: relative;
  display: flex;
  align-items: center;
  height:100rpx;
  font-size: 35rpx;
  padding:10rpx;
  padding-left: 30rpx;
  margin: 15rpx;
  color: #888;
  background-color: #f4f4f4;
  border:1rpx dotted #eb4450;
  border-radius: 10rpx;
}
#share{
  z-index: 2;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.background{
  z-index: 0;
  width: 100%;
  height: 90vh;
  position: absolute;
  top: 0;
  left: 0;
	margin: 0;
	opacity: 0.5;
}
.log{
  z-index: 2;
  height: 40vh;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}
.log1{
  width: 70%;
  z-index: 2;
  position: relative;
  height: 200rpx;
}
.logged{
  z-index: 3;
  position:absolute;
  height: 200rpx;
  opacity: 0;
}
.exitlog{
  z-index: 2;
  height: 90rpx;
  display: flex;
  justify-content: center;
}
.confirm-btn {
  font-size: 13pt;
  line-height: 90rpx;
  height:90rpx;
  background: #eb4450;
  color: #fff;
  text-align: center;
  border-radius: 100rpx;
  margin-top: 40rpx;
  width: 50%;
}
.goto{
 font-size: 13pt;
  line-height: 60rpx;
  height: 90rpx;
  background: #eb4450;
  color: #fff;
  text-align: center;
  border-radius: 100rpx;
  margin-top:50rpx;
}
